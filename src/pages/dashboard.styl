section
   position: relative;
   max-width: 1710px;
   height: 100vh;
   margin: 0 auto; 
   display: flex;

   .container
      width: calc(100% - 106px);
      #overlay
         position: fixed;
         top: 0;
         left: 0;
         width: 100%;
         height: 100vh;
         background-color: rgba(70, 93, 118, 0.555);
         z-index: 20;
      
      .cont-main
         display: flex;
         width: 100%;
         height: calc(100vh - 83px);

